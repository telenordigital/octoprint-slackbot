{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "airbnb-base",
    "rules": {
        "no-underscore-dangle": ["off"],
        "indent": ["error", 4]
    }
}